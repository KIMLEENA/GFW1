<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:iconSpaceReserved="false">
    <PreferenceCategory
        android:title="정보"
        app:iconSpaceReserved="false">
        <PreferenceScreen
            android:summary="버전 정보를 확인할 수 있습니다."
            android:title="버전 정보"
            app:iconSpaceReserved="false">
            <intent
                android:targetClass="com.example.capstonedesign.InformationActivity"
                android:targetPackage="com.example.capstonedesign" />
        </PreferenceScreen>
        <PreferenceScreen
            android:summary="프로필 정보를 확인할 수 있습니다."
            android:title="프로필"
            app:iconSpaceReserved="false">
            <intent
                android:targetClass="com.example.capstonedesign.imageActivity"
                android:targetPackage="com.example.capstonedesign" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="알림"
        app:iconSpaceReserved="false">
        <PreferenceScreen
            android:title="공지 사항"
            app:iconSpaceReserved="false">
            <intent
                android:targetClass="com.example.capstonedesign.NoticeActivity"
                android:targetPackage="com.example.capstonedesign" />
        </PreferenceScreen>
        <PreferenceScreen
            android:title="도움말"
            app:iconSpaceReserved="false">
            <intent
                android:targetClass="com.example.capstonedesign.QuestionActivity"
                android:targetPackage="com.example.capstonedesign" />
        </PreferenceScreen>
    </PreferenceCategory>
    <PreferenceCategory></PreferenceCategory>
    <PreferenceScreen
        android:title="로그아웃"
        app:iconSpaceReserved="false">
        <intent
            android:targetClass="com.example.capstonedesign.Logout"
            android:targetPackage="com.example.capstonedesign" />
    </PreferenceScreen>
    <PreferenceScreen
        android:title="회원탈퇴"
        app:iconSpaceReserved="false">
        <intent
            android:targetClass="com.example.capstonedesign.Delete"
            android:targetPackage="com.example.capstonedesign" />
    </PreferenceScreen>

</PreferenceScreen>